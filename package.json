{
  "name": "ten",
  "version": "1.0.0",
  "description": "TypeScript + Express.js + Node",
  "author": "Konrad Gadzinowski <kgadzinowski@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch src --watch dist --watch test --ext ts,tsx,js,scss,css --exec '\"ts-node\" -- \"src/server/app/BootDev.ts\"'",
    "start-prod": "\"ts-node\" \"./src/server/app/BootProd.ts\"",
    "build-client": "gulp build-client",
    "build-client-dev": "gulp build-client-dev",
    "dev-client": "gulp dev-client",
    "format-code": "prettier --config .prettierrc \"{src/**/*,test/**/*}.{ts,tsx,css,less,scss,js,jsx}\" --write",
    "test-server": "mocha -r ts-node/register src/server/**/*.test.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "body-parser": "~1.19.1",
    "cookie-parser": "~1.4.6",
    "css-loader": "^6.5.1",
    "debug": "~4.3.3",
    "express": "~4.17.2",
    "gulp-run-sequence": "^0.3.2",
    "hbs": "~4.2.0",
    "less": "^4.1.2",
    "morgan": "~1.10.0",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "serve-favicon": "~2.5.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/html": "^6.4.9",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.10",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.6",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-nodemon": "^2.5.0",
    "jasmine-core": "^3.10.1",
    "jsdom": "^19.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack-stream": "^7.0.0"
  }
}
